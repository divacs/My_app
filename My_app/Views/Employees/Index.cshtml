@model List<Employee>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Total Hours Worked</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
        <tr @(employee.TotalHoursWorked < 100 ? "class='less-than-100'" : "")>

            <td>@employee.Name</td>
            <td>@employee.TotalHoursWorked</td>
        </tr>
        }
    </tbody>
</table>
